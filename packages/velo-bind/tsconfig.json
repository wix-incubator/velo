{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES6",
    "lib": ["ES2019", "DOM"],
    "moduleResolution": "node",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "sourceMap": true
  }
}
